        "\n",
        "number = int(input(\"Enter a number (1 to 1000): \"))\n",
        "\n",
        "if number < 1 or number > 1000:\n",
        "    print(\"Number must be between 1 and 1000.\")\n",
        "else:\n",
        "    print(\" In words:\", p.number_to_words(number).capitalize())\n"
